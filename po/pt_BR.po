msgid "Running: \"%1\"\n"
msgstr "Executando: \"%1\"\n"

#: ../install-menu/install-menu.cc:1418
msgid "install-menu: %1: aborting\n"
msgstr "install-menu: %1: abortando\n"

#: ../install-menu/install-menu.h:368
msgid "Number of arguments to function %1 does not match."
msgstr "Número de argumentos para a função %1 não bate."

#: ../install-menu/install-menu.h:375
msgid "Unknown function: \"%1\""
msgstr "Função desconhecida: \"%1\""

#: ../install-menu/install-menu.h:383
msgid "Indirectly used, but not defined function: \"%1\""
msgstr "Função indiretamente usada, mas não definida: \"%1\""

#: ../install-menu/install-menu.h:391
msgid "Unknown identifier: \"%1\""
msgstr "Identificador desconhecido: \"%1\""

#: ../install-menu/install-menu.h:399
msgid "Could not open directory \"%1\"."
msgstr "Não foi possível abrir o diretório \"%1\"."

#: ../install-menu/install-menu.h:407
msgid ""
"Missing (or empty) tag: %1\n"
"This tag needs to be defined for the menu entry to make sense.\n"
"Note that update-menus rearranges the order of the tags found\n"
"in the menu-entry files, so that the part above isn't literal."
msgstr ""
"Tag faltando (ou vazia): %1\n"
"Essa tag precisa estar definida para a entrada de menu ter sentido.\n"
"Note que o update-menus rearranja a ordem das tags encontradas\n"
"nos arquivos de entrada de menu para que a parte acima não seja\n"
"literal."

#: ../install-menu/install-menu.h:419
msgid "Encoding conversion error: \"%1\""
msgstr "Erro de conversão de codificação: \"%1\""

